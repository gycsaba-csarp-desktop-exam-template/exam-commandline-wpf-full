<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:KretaDesktop.View"
                    xmlns:viewmodels="clr-namespace:KretaDesktop.ViewModel"
                    xmlns:viewmodelscontent="clr-namespace:KretaDesktop.ViewModel.Content">
    <DataTemplate DataType="{x:Type viewmodels:HeaderAppNameViewModel}">
        <views:HeaderAppName></views:HeaderAppName>
    </DataTemplate>
    
    
    <DataTemplate DataType="{x:Type viewmodels:HeaderListViewModel}">
        <views:HeaderListView></views:HeaderListView>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:HeaderOperationViewModel}">
        <views:HeaderOperationView></views:HeaderOperationView>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:HeaderAppConfigurationViewModel}">
        <views:HeaderAppConfigurationView></views:HeaderAppConfigurationView>
    </DataTemplate>


    <DataTemplate DataType="{x:Type viewmodelscontent:ContentListSubjectViewModel}">
        <views:ContentListSubjectView></views:ContentListSubjectView>
    </DataTemplate>
</ResourceDictionary>